VAR
	M1 : ExternalStaticMatrix;
	M2 : DynamicMatrix;
	Data : ARRAY[0..8] OF LREAL;
	
	
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST('WhenCreatedExpectDataAssignable');

// @TEST-RUN
M2.InitMagicSquare(3);
M1.Init(ADR(Data),3,3);
M1.FillFrom(M2);


// @TEST-ASSERT
AssertTrue(M1.IsEqual(M2),'Data was not all assigned');

TEST_FINISHED();]]></ST>
